{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "framework": "vite",
    "env": {
        "SUPABASE_URL": "@supabase_url",
        "SUPABASE_KEY": "@supabase_key",
        "GEMINI_API_KEY": "@gemini_api_key",
        "DEFAULT_USER_ID": "@default_user_id"
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        }
    ]
}